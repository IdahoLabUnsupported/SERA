# sets up the enviroment varaiables for BNCT. There are
# several ways to make this file take effect. See your system
# administrator for the best way.
setenv BNCT_HOME $HOME/bnct3.0

# BNCT_RESOURCES locates resource information required by BNCT and
# its ancillary functions like doseplay. Also contains materials and
# color files.
setenv BNCT_RESOURCES $BNCT_HOME/Resources

#set documentation pathes
#setenv FILES_MANUAL_PATH $BNCT_RESOURCES/manual/
#setenv NET_MANUAL_PATH http://id.inel.gov/cart/rtpe-manual/
setenv BNCT_HELP $BNCT_HOME/Docs/dialog_help
setenv FILES_MANUAL_PATH $BNCT_HOME/Docs/manual/
setenv NET_MANUAL_PATH http://id.inel.gov/cart/rtpe-manual/

#Floyd
setenv RTT RttMC
setenv RTT_PATH $BNCT_HOME/$RTT
